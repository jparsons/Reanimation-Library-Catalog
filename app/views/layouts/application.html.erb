    <div class="sidebar-menu-item">
      <%= link_to "add a new item", new_item_path %>
    </div>
    <div class="sidebar-menu-item">
      <%= link_to "view items that need cataloging", acquired_items_path %>
    </div>
    <div class="sidebar-menu-item">
      <%= link_to "view items that have no images", need_images_items_path %>
    </div>
  <% end %>
