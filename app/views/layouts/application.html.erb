</a>
<% end %>
<% if current_page? by_call_number_items_path %>
  <div class="ami1"><div class="ami3"><p class="ami3">BROWSE CATALOG<br />BY CALL NUMBER</p></div></div>
<% else %>
  <a href="<%= by_call_number_items_path %>">
    <div class="ami1"><div class="ami2"><p class="ami2">BROWSE CATALOG<br/>BY CALL NUMBER</p></div></div>
</a>
<% end %>
<% if current_page? recent_items_path %>
  <div class="ami1"><div class="ami3"><p class="ami3">VIEW RECENTLY<br/>ADDED ITEMS</p></div></div>
<% else %>
