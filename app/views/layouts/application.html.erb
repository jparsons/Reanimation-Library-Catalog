    <div class="sidebar-menu-item">
        <%= link_to "edit images", digital_assets_path %>
    </div>
  <% end %>
  <% if logged_in? && current_user.is_administrator? %>

    <div class="sidebar-menu-item">
        <%= link_to "add a new exhibition", new_exhibition_path %>
    </div>
    <div class="sidebar-menu-item">
        <%= link_to "add a new art work", new_work_path %>
    </div>
