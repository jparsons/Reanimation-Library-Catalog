<%- content_for(:title) { "Reanimation Library - Image Archive" } -%>
<h2>Images</h2>
<%= raw will_paginate @digital_assets %>
<% @digital_assets.each do |asset| %>
<%= link_to image_tag(asset.scan.url(:thumb)), digital_asset_path(asset), :class=>"digital-asset" %>
<% end %>
<br style="clear:both" />
<%= raw will_paginate @digital_assets %>
