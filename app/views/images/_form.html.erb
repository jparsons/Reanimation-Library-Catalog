<% form_for @image do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :item %><br />
    <%= f.text_field :item %>
  </p>
  <p>
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </p>
  <p>
    <%= f.label :image_type %><br />
    <%= f.text_field :image_type %>
  </p>
  <p>
    <%= f.label :legacy_id %><br />
    <%= f.text_field :legacy_id %>
  </p>
  <p>
    <%= f.label :is_marked %><br />
    <%= f.text_field :is_marked %>
  </p>
  <p><%= f.submit %></p>
<% end %>
