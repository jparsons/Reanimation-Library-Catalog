<h1>These Items Need Images</h2>
  <table id="item-list">
    <tr>
      <td class="call-number">Call Number</th><td class="title">Title</th>
    </tr>
	<% @items.each do |item| %>
	  <tr class="<%= cycle("odd", "even") %>">
	    <td><%= item.call_number.blank? ? "[blank]" : item.call_number %></td><td><%= item.display_title %></td>
	  </tr>
	<% end %>
	</table>